{
  "name": "sky-vite-template",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "release": "standard-version",
    "lint": "eslint src --max-warnings=0",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "test": "jest",
    "test:watch": "yarn jest --watch",
    "docs": "typedoc --entryPointStrategy  expand --tsconfig ./tsconfig.json --excludeInternal --excludeProtected",
    "docs-hierarchy": "typedoc --entryPointStrategy  expand --tsconfig ./tsconfig.json --plugin ./node_modules/typedoc-theme-hierarchy/dist/index.js --theme hierarchy",
    "docs-github": "typedoc --theme github-wiki --entryPointStrategy  expand --tsconfig ./tsconfig.json  "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --passWithNoTests"
    ]
  },
  "dependencies": {
    "preact": "^10.8.2",
    "react": "^16.8",
    "react-dom": "^16.8",
    "react-is": "^16.8",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@preact/preset-vite": "^2.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/next": "^9.0.0",
    "@types/node": "^17.0.14",
    "@types/react": "^18.0.14",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "^6.9.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.13",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^13.0.1",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "release": "^6.3.0",
    "standard-version": "^9.5.0",
    "storybook-addon-next-router": "^3.0.8",
    "ts-node": "^10.8.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.13",
    "webpack": "5.68.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
